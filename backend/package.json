    "words:upload": "node scripts/prepare_words.cjs --upload"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240925.0",
    "typescript": "^5.6.3",
    "wrangler": "^4.42.2"
  }
}
